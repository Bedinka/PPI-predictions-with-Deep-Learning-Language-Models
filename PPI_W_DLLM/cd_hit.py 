from Bio.PDB import PDBParser
from Bio.SeqUtils import seq1

def pdb_to_fasta(pdb_file):
    parser = PDBParser()
    structure = parser.get_structure('structure', pdb_file)
    sequence_A = ''
    sequence_B = ''
    for model in structure:
        for chain in model:
            if chain.get_id() == 'A':
                for residue in chain:
                    if residue.get_id()[0] == ' ':
                        sequence_A += seq1(residue.get_resname())
            elif chain.get_id() == 'B':
                for residue in chain:
                    if residue.get_id()[0] == ' ':
                        sequence_B += seq1(residue.get_resname())
    return sequence_A, sequence_B

pdb_file = 'your_interactom.pdb'
sequence_A, sequence_B = pdb_to_fasta(pdb_file)

# Write the sequences to a FASTA file
with open('interactom.fasta', 'w') as f:
    f.write('>Chain_A\n')
    f.write(sequence_A + '\n')
    f.write('>Chain_B\n')
    f.write(sequence_B + '\n')
